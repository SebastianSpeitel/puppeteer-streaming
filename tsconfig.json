{
    "compilerOptions": {
        "module": "commonjs",
        "lib": [
            "ESNext"
        ],
        "target": "ES2019",
        "alwaysStrict": true,
        "noImplicitAny": true,
        "noEmitOnError": true,
        "noImplicitReturns": true,
        // "noUnusedLocals": true,
        "noUnusedParameters": true,
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules"
    ]
}